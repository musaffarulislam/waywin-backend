{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "outDir": "./dist",
    "rootDir": "./src",  
    "baseUrl": ".",  
    "sourceMap": true,
    "pretty": true,
    "allowJs": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"], 
  "exclude": [
    "node_modules"
  ]
}


